#counting basepairs
counter = {'AA':0,'AC':0,'AG':0,'AT':0,
          'CA':0,'CC':0,'CG':0,'CT':0,
          'GG':0,'GA':0,'GC':0,'GT':0,
          'TT':0,'TA':0,'TC':0,'TG':0}
for line_num,line in enumerate(nd2.fasta.txt):
   if line_num%4 ==1:
       line = line.rstrip()
       for i,base in enumerate(line[:-1]):
           kmer = line[i:i+3]
           counter[kmer] +=1
print(counter)


def count_kmers(self,k):
   counter = {}
   for i,base in enumerate(self.sequence): 
       kmer = self.sequence[i:i+k] 
       if kmer in counter:
           counter[kmer] += 1 
           else:
               counter[kmer] = 1 
       return(counter)
